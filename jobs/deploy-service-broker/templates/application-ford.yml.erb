broker:
  management-endpoint: <%= properties.managementendpoint %>
  namespace: <%= properties.namespace %>
  object-endpoint: <%= properties.objectendpoint %>
  repository-endpoint: <%= properties.repositoryendpoint%>
  replication-group: <%= properties.replicationgroup %>
  certificate: ecs_cert.pem
  base-url: <%= properties.baseurl %>
  repository-user: <%= properties.repositoryuser%>
  username: <%= properties.username%>
  password: <%= properties.password%>
  repository-bucket: <%= properties.repositorybucket %>
  prefix: <%= properties.prefix %>
  repository-service-id: f3cbab6a-5172-4ff1-a5c7-72990f0ce2aa
  repository-plan-id: 89d20694-9ab0-4a98-bc6a-868d6d4ecf31
security:
  user:
    password: <%= properties.securitypassword %>
catalog:
  services:
    - id: <%= properties.catalog_services_id %>
      name: <%= properties.catalog_services_name %>
      description:<%= properties.catalog_services_description %>
      bindable: <%= properties.catalog_services_bindable %>
      plan-updatable: <%= properties.catalog_services_plan_updatable %>
      tags:
        - <%= properties.catalog_services_tag_1 %>
        - <%= properties.catalog_services_tag_2 %>
      service-settings:
        service-type: <%= properties.catalog_services_service_settings_service_type %>
        head-type: <%= properties.catalog_services_service_settings_head_type %>
        access-during-outage: <%= properties.catalog_services_service_settings_access_during_outage %>
        encrypted: <%= properties.catalog_services_service_settings_encrypted %>
        file-accessible: <%= properties.catalog_services_service_settings_file_accessible %>
        file-system-enabled: <%= properties.catalog_services_service_settings_file_system_enabled %>
      metadata:
        displayName: <%= properties.catalog_services_metadata_displayName %>
        imageUrl: <%= properties.catalog_services_metadata_imageUrl %>
        longDescription: <%= properties.catalog_services_metadata_longDescription %>
        providerDisplayName: <%= properties.catalog_services_metadata_providerDisplayName %>
        documentationUrl: <%= properties.catalog_services_metadata_documentationUrl %>
        supportUrl: <%= properties.catalog_services_metadata_supportUrl %>
      plans:
        - id: <%= properties.catalog_services_plans_id %>
          name: <%= properties.catalog_services_plans_name %>
          description: <%= properties.catalog_services_plans_description%>
          service-settings:
            quota:
              limit: <%= properties.catalog_services_plans_service_settings_quota_limit %>
              warn: <%= properties.catalog_services_plans_service_settings_quota_warn %>
		  free: <%= properties.catalog_services_plans_free %>	  
          metadata:
            costs:
              - amount:
                  usd: <%= properties.catalog_services_plans_metadata_costs_amount_usd %>
                unit: <%= properties.catalog_services_plans_metadata_costs_unit %>
            bullets:
              - <%= properties.catalog_services_plans_metadata_bullet_1 %>
              - <%= properties.catalog_services_plans_metadata_bullet_2 %>
              - <%= properties.catalog_services_plans_metadata_bullet_3 %>
        - id: <%= properties.catalog_services_plans_id1 %>
          name: <%= properties.catalog_services_plans_name1 %>
          description: <%= properties.catalog_services_plans_description1 %>
          service-settings:
            quota:
              limit: <%= properties.catalog_services_plans_service_settings1_quota1_limit1 %>
              warn: <%= properties.catalog_services_plans_service_settings1_quota1_warn1 %>
		  free: <%= properties.catalog_services_plans_free1 %>
          metadata:
            costs:
              - amount:
                  usd: <%= properties.catalog_services_plans_metadata1_costs1_amount1_usd1 %>
                unit: <%= properties.catalog_services_plans_metadata1_costs1_unit1 %>
            bullets:
              - <%= properties.catalog_services_plans_metadata1_bullets1_1 %>
              - <%= properties.catalog_services_plans_metadata1_bullets1_2 %>
              - <%= properties.catalog_services_plans_metadata1_bullets1_3 %>
